<h2> Time Entries From All Time!</h2>
<table>
  <thead>
    <tr>
      <th>Start time</th>
      <th>Stop time</th>
      <th>Total time (seconds)</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_entries.each do |time_entry| %>
      <tr>
        <td><%= time_entry.start_time %></td>
        <td><%= time_entry.stop_time %></td>
        <td><%= distance_of_time_in_words(time_entry.stop_time, time_entry.start_time, except: :seconds) %></td>
        <td><%= time_entry.date %></td>
        <td><%= link_to 'Show', time_entry %></td>
        <td><%= link_to 'Edit', edit_time_entry_path(time_entry) %></td>
        <td><%= link_to 'Destroy', time_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
