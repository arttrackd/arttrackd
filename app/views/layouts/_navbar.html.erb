<header class="navigation" role="banner">
  <div class="navigation-wrapper">
    <a href="javascript:void(0)" class="navigation-menu-button" id="js-mobile-menu">MENU</a>
    <nav role="navigation">
      <ul id="js-navigation-menu" class="navigation-menu show">
        <% if logged_in? %>
          <li class="nav-link"><%= link_to 'ArtTrack Home', root_path %></li>

        
          <li class="nav-link more"><%= link_to 'Goals', 'javascript:void(0)' %>
            <ul class="submenu">
              <li><%= link_to 'New Sales Goal', new_sales_goal_path %></li>
              <li><%= link_to 'All Sales Goals', sales_goals_path %></li>
            </ul>
          </li>
          <% if clocked_in? %>
            <div class = "stopwatch-text">I have been working on <%= @current_time_entry.project.name %> for
            <div class="stopwatch" id="timer" start-time="<%=@current_time_entry.start_time.to_f * 1000 %>"></div>
            <li class="nav-link"><%= link_to 'Clock Out', clock_out_time_entries_path(time_entry: {project_id: @current_time_entry.project_id}), method: :patch, id: "clock-out-nav" %></li>
          <% end %>
          <li class="nav-link"><%= link_to 'Profile', user_path(id: session[:user_id]) %></li>
          <li class="nav-link"><%= link_to 'Log Out', logout_path, method: :delete %></li>
        <% else %>
          <li class="nav-link"><%= link_to 'ArtTrack', root_path %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>
